{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/"
      ],
      "startServerCommand": "npm run start",
      "numberOfRuns": 3
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 1
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 1
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 1
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 1
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
